{
  "version": 2,
  "builds": [
    {
      "src": "startup/index.html",  
      "use": "@vercel/static-build",
      "config": { "outputDirectory": "dist" }
    },
    {
      "src": "serverbrow/index.js",    
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",       
      "dest": "serverbrow/index.js" 
    },
    {
      "src": "/(.*)",           
      "dest": "startup/index.html" 
    }
  ]
}
